TRIGGER ShipDraw {
DRAW ELEMENT:Clear OBJECT $0 $1 # ELEMENT:OmniCruiserBeta H ELEMENT:OmniCruiserBeta2 {
	" HHHH "
	"HH##HH"
	"H H HH H H"
	"#HHHH#"
	"  HH  "
	"HH##HH"
	" HHHH "
	}
}

SET startX 173
SET startY 175
SET heath 2
SET abletomove 1
TRIGGER Move {
	EXEC ShipClear startX startY
	IF ((bluesabbed == 0) && (startY > 6) && ($0 == 1)) <SET startY (startY - 2)>
	IF ((bluesabbed == 0) && (startY < (HEIGHT - 7)) && ($0 == 2)) <SET startY (startY + 2)>
	IF ((bluesabbed == 0) && (startX > 2) && ($0 == 3)) <SET startX (startX - 2)>
	IF ((bluesabbed == 0) && (startX < (WIDTH - 6)) && ($0 == 4)) <SET startX (startX + 2)>
	IF ((bluesabbed == 1) && (startY > 6) && ($0 == 1)) <SET startY (startY - 1)>
	IF ((bluesabbed == 1) && (startY < (HEIGHT - 7)) && ($0 == 2)) <SET startY (startY + 1)>
	IF ((bluesabbed == 1) && (startX > 2) && ($0 == 3)) <SET startX (startX - 1)>
	IF ((bluesabbed == 1) && (startX < (WIDTH - 6)) && ($0 == 4)) <SET startX (startX + 1)>
	EXEC ShipDraw startX startY
	IF (($0 == 1) && (KEYwUP == 1) && (abletomove == 1)) <TIMER 2 FRAMES Move 1>
	IF (($0 == 2) && (KEYsUP == 1) && (abletomove == 1)) <TIMER 2 FRAMES Move 2>
	IF (($0 == 3) && (KEYaUP == 1) && (abletomove == 1)) <TIMER 2 FRAMES Move 3>
	IF (($0 == 4) && (KEYdUP == 1) && (abletomove == 1)) <TIMER 2 FRAMES Move 4>
}

TRIGGER BLaser {
	IF (superblueBLaser == 0) {
		CLOSEST GROUP:Galaxy $0 $1 (WIDTH MIN HEIGHT)
		IF (CLOSESTD < 50) {
			DRAW ELEMENT:BLaser LINE $0 $1 (CLOSESTX - $0) (CLOSESTY - $1)
			DRAW ELEMENT:Explosion FILLEDCIRCLE CLOSESTX CLOSESTY 3
			SET heath (heath - 1)
			IF ((heath <= 0) && (abletomove == 1)) resetship
		
		}
	}
	IF (superblueBLaser == 1) {
		CLOSEST GROUP:Galaxy $0 $1 (WIDTH MIN HEIGHT)
		IF (CLOSESTD < 500) {
			DRAW ELEMENT:BLaser LINE $0 $1 (CLOSESTX - $0) (CLOSESTY - $1)
			DRAW ELEMENT:Explosion FILLEDCIRCLE CLOSESTX CLOSESTY 10
		}
	}
}

