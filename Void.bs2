// For Fyron
// ELEMENT that starts it all
ELEMENT Shooting OmegaS.S.C.CannonStarter 255 255 255 0 0 0 1 7000000 Clear 10 HEX 400000006300000011E00000083000000C0800000600020002070200018F01000032010000260080001C708000CCD0800C9B904006F320400000E1C00000E0E00001B0F0000618B000000CB9008006BB00C0037F00607FDE001C18DC0007F91C00000F78000003B0000000E0000001C00000038000003E000000700000004000 217 255 0
// Decor
ELEMENT None OmegaS.S.C.Cannon 255 255 0 0 0 0 1 100% OmegaS.S.C.Cannon2 10 Text 
ELEMENT None Pellet 205 105 0 0 0 0 1 800000 Clear 10 Text None
ELEMENT None OmegaS.S.C.Cannon2 255 255 0 0 0 0 1 1000 Clear 10 Text None
INTERACTIONTRIGGER OmegaS.S.C.CannonStarter Clear S.S.C.ThreeSixty 10%
// INTERACTIONTRIGGER S.S.C.ThreeSixty All Fire 100% Clear
// SET DrawType 1  
// ReplaceElement:Clear
// Important

TRIGGER S.S.C.ThreeSixty {
   IF (($0 > -10) && (Fighting == 1)) {
      SET tmpX (X2 - (0 RAND WIDTH))
      SET tmpY (Y2 - (0 RAND HEIGHT))
      SET tmp (SQRT ((tmpX * tmpX) + (tmpY * tmpY)))
      SET $7 1
      SET $8 1

      

   }
   EXEC S.S.C.ThreeSixty2 (X2 * 50) (Y2 * 50) ((tmpX * (1 * -50)) / tmp) ((tmpY * (1 * -50)) / tmp) ELEMENT:Pellet 1 ELEMENT:Clear 1 0 1
}

TRIGGER S.S.C.ThreeSixty2 {

IF (Fighting == 1) {

   SET Ionx (($0 + $2) / 50)
   SET Iony (($1 + $3) / 50)
   IF ((Ionx > 0) && (Ionx < WIDTH) && (Iony > 0) && (Iony < HEIGHT)) {
      DRAW $4 POINT Ionx Iony $5 $6 
      TIMER 1 FRAMES S.S.C.ThreeSixty2 ($0 + $2) ($1 + $3) $2 $3 $4 $5 $6 $7 $8 1
   }
}
		}


