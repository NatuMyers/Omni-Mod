TRIGGER Wind {
         WindCycle = (0 RAND 8)
   IF ((X != 0) && (Y != 0)) {
      IF ((WindMove >= 1) && (WindMotion == 1)) {
               IF ((X1 < X) && (Y1 < Y) && (WindCycle == 0)) {
                  IF (((X1 + WindMove) PIXEL (Y2 + WindMove)) == ELEMENT:Clear) {
                        DRAW 0 SWAPPOINTS X1 Y1 (X1 + WindMove) (Y2 + WindMove)
                  }
               }
               IF ((X1 < X) && (Y1 > Y) && (WindCycle == 1)) {
                  IF (((X1 + WindMove) PIXEL (Y2 - WindMove)) == ELEMENT:Clear) {
                       DRAW 0 SWAPPOINTS X1 Y1 (X1 + WindMove) (Y2 - WindMove)
                  }
              }
               IF ((X1 > X) && (Y1 < Y) && (WindCycle == 2)) {
                  IF (((X1 - WindMove) PIXEL (Y2 + WindMove)) == ELEMENT:Clear) {
                        DRAW 0 SWAPPOINTS X1 Y1 (X1 - WindMove) (Y2 + WindMove)
                  }
               }
               IF ((X1 > X) && (Y1 > Y) && (WindCycle == 3)) {
                  IF (((X1 - WindMove) PIXEL (Y2 - WindMove)) == ELEMENT:Clear) {
                        DRAW 0 SWAPPOINTS X1 Y1 (X1 - WindMove) (Y2 - WindMove)
                  }
               }
               IF ((Y1 == Y) && (WindCycle == 4)) {
                  IF ((X1 PIXEL (Y2 + WindMove)) == ELEMENT:Clear) {
                        DRAW 0 SWAPPOINTS X1 Y1 X1 (Y2 + WindMove)
                  }
               }
               IF ((Y1 == Y) && (WindCycle == 5)) {
                  IF ((X1 PIXEL (Y2 - WindMove)) == ELEMENT:Clear) {
                       DRAW 0 SWAPPOINTS X1 Y1 X1 (Y2 - WindMove)
                  }
              }
               IF ((X1 == X) && (WindCycle == 6)) {
                  IF (((X1 - WindMove) PIXEL Y2) == ELEMENT:Clear) {
                        DRAW 0 SWAPPOINTS X1 Y1 (X1 - WindMove) Y2
                  }
               }
               IF ((X1 == X) && (WindCycle == 7)) {
                  IF (((X1 + WindMove) PIXEL Y2) == ELEMENT:Clear) {
                        DRAW 0 SWAPPOINTS X1 Y1 (X1 + WindMove) Y2
                  }
               }
      }
      IF ((WindMove >= 1) && (WindMotion == 2)) {
               IF ((X1 > X) && (Y1 > Y) && (WindCycle == 0)) {
                  IF (((X1 + WindMove) PIXEL (Y2 + WindMove)) == ELEMENT:Clear) {
                        DRAW 0 SWAPPOINTS X1 Y1 (X1 + WindMove) (Y2 + WindMove)
                  }
               }
               IF ((X1 > X) && (Y1 < Y) && (WindCycle == 1)) {
                  IF (((X1 + WindMove) PIXEL (Y2 - WindMove)) == ELEMENT:Clear) {
                       DRAW 0 SWAPPOINTS X1 Y1 (X1 + WindMove) (Y2 - WindMove)
                  }
              }
               IF ((X1 < X) && (Y1 > Y) && (WindCycle == 2)) {
                  IF (((X1 - WindMove) PIXEL (Y2 + WindMove)) == ELEMENT:Clear) {
                        DRAW 0 SWAPPOINTS X1 Y1 (X1 - WindMove) (Y2 + WindMove)
                  }
               }
               IF ((X1 < X) && (Y1 < Y) && (WindCycle == 3)) {
                  IF (((X1 - WindMove) PIXEL (Y2 - WindMove)) == ELEMENT:Clear) {
                        DRAW 0 SWAPPOINTS X1 Y1 (X1 - WindMove) (Y2 - WindMove)
                  }
               }
               IF ((Y1 == Y) && (WindCycle == 4)) {
                  IF ((X1 PIXEL (Y2 + WindMove)) == ELEMENT:Clear) {
                        DRAW 0 SWAPPOINTS X1 Y1 X1 (Y2 + WindMove)
                  }
               }
               IF ((Y1 == Y) && (WindCycle == 5)) {
                  IF ((X1 PIXEL (Y2 - WindMove)) == ELEMENT:Clear) {
                       DRAW 0 SWAPPOINTS X1 Y1 X1 (Y2 - WindMove)
                  }
              }
               IF ((X1 == X) && (WindCycle == 6)) {
                  IF (((X1 - WindMove) PIXEL Y2) == ELEMENT:Clear) {
                        DRAW 0 SWAPPOINTS X1 Y1 (X1 - WindMove) Y2
                  }
               }
               IF ((X1 == X) && (WindCycle == 7)) {
                  IF (((X1 + WindMove) PIXEL Y2) == ELEMENT:Clear) {
                        DRAW 0 SWAPPOINTS X1 Y1 (X1 + WindMove) Y2
                  }
               }
      }
   WindMove = (((7 - (((SQRT ((X1 - X) ^ 2) + ((Y1 - Y) ^ 2)) / 10000) + WindInc)) * WindNess) * -1)
   } ELSE <WindMove = ((7 - (((SQRT ((X1 - X) ^ 2) + ((Y1 - Y) ^ 2)) / 10000) + WindInc)) * WindNess)>
}

