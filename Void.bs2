TRIGGER rcAnimate {
	EXEC CheckSize 
	EXEC MenuMove
	IF (Universetype == 0) eraseSideElements 
	EXEC drawfrontside (WIDTH / 2) (HEIGHT / 2) 1
	EXEC edgesandsides
	EXEC recenterP
	IF (AScheck == 0) DoUniverseAction 
	EXEC movesection
	IF (sizepreviewtime > 0) PreviewCount 
	TIMER 2 FRAMES rcAnimate 
}




TRIGGER Universemenubottom {

	MENU BOTTOM CLEAR
	EXEC fileOptions
	IF ((showCursormenu == 0) && (showFilemenu == 0)) addmenuspaces 128
	EXEC addmenuspaces menuSeperator
	EXEC cursorOptions
	MENU BOTTOM ADD TEXT " Universe Options " FILE http://uploadpicz.com/images/YG8K68R.bmp Universeoptionsmenu
	EXEC gameOptions
	EXEC addmenuspaces menuSeperator
	IF ((showOptionsmenu == 0) && (showAutomenu == 0)) addmenuspaces 96
	IF ((showOptionsmenu == 0) && (showAutomenu == 0) && (Universeaction == 0)) addmenuspaces 32
	IF ((showOptionsmenu == 1) && (Universeaction != 0)) <MENU BOTTOM ADD TEXT "" SEPERATOR -32 <>>
	EXEC automixRe-arrangemenu
}

TRIGGER Universeoptionsmenu {
	SET maxsize ((WIDTH / 8) MIN (HEIGHT / 8))
	SET sizerange ((maxsize - minsize) MAX 1)
	SET showval ((((((Universesize - minsize) * 32) / sizerange) + startsize) MAX startsize) MIN (startsize + 31))

	MENU SUB CLEAR 

	IF (showMenusmenu == 1) {
		MENU SUB ADD TEXT " " FILE http://uploadpicz.com/images/SH4EFQ4.bmp showhideMenus
		MENU SUB ADD TEXT " Options " ELEMENT ELEMENT:filemenuicon {
			EXEC showhidefile
			EXEC Universeoptionsmenu
		}
		MENU SUB ADD TEXT " Cursor " ELEMENT ELEMENT:cursoroptionsicon {
			EXEC showhidecursor
			EXEC Universeoptionsmenu
		}
		MENU SUB ADD TEXT " Options " ELEMENT ELEMENT:gameoptions {
			EXEC showhideoptions 
			EXEC Universeoptionsmenu
		}
		MENU SUB ADD TEXT " Mix / Re-arrange " ELEMENT ELEMENT:mixRe-arrange {
			EXEC showhideauto
			EXEC Universeoptionsmenu
		}
		EXEC addsubmenuspaces 28
	}
	IF (showMenusmenu == 0) <MENU SUB ADD TEXT " " FILE http://uploadpicz.com/images/T5ZJZ6S.bmp showhideMenus>
	MENU SUB ADD TEXT " Universe Type " ELEMENT ELEMENT:Universetypeicon Universetypemenu
	IF (Universetype == 0) <MENU SUB ADD TEXT " Universe Colors " ELEMENT ELEMENT:UniverseColorS Universecolorsmenu>
	IF ((Universetype == 1) | (Universetype == 2)) <MENU SUB ADD TEXT " Sand Colors " ELEMENT ELEMENT:SandColorS Universecolorsmenu>
	IF (Universetype == 3) <MENU SUB ADD TEXT " Universe Elements " ELEMENT ELEMENT:UniverseElementS UniverseElementMenu 0>
	MENU SUB ADD TEXT "" SEPERATOR 4 <>
	MENU SUB ADDDRAG TEXT " Size " ELEMENT showval setUniversesize ((((X + 1) * sizerange) / 4 / 8) + minsize)
	MENU SUB ADD TEXT "" SEPERATOR 8 <>
	EXEC addsubmenuspaces 64
	SUBMENU
}

TRIGGER addsubmenuspaces {
	FOR tmpsubspc FROM 1 TO $0 DO {
		MENU SUB ADD TEXT "" FILE http://uploadpicz.com/images/ASR6YYZ.bmp <>
	}

}
TRIGGER addmenuspaces {
	FOR tmpmnuspc FROM 1 TO $0 DO {
		MENU BOTTOM ADD TEXT "" FILE http://uploadpicz.com/images/L8K828T.bmp <>
	}
}

TRIGGER showhideMenus {
	SET showMenusmenu (1 - showMenusmenu)
	EXEC Universeoptionsmenu
}

TRIGGER showhidefile {
	SET showFilemenu (1 - showFilemenu)
	IF (showFilemenu == 1) <SET showCursormenu 0>
	EXEC Universemenubottom 
}
TRIGGER showhidecursor {
	SET showCursormenu (1 - showCursormenu)
	IF (showCursormenu == 1) <SET showFilemenu 0>
	EXEC Universemenubottom 
}
TRIGGER showhideoptions {
	SET showOptionsmenu (1 - showOptionsmenu)
	IF (showOptionsmenu == 1) <SET showAutomenu 0>
	EXEC Universemenubottom 
}
TRIGGER showhideauto {
	SET showAutomenu (1 - showAutomenu)
	IF (showAutomenu == 1) <SET showOptionsmenu 0>
	EXEC Universemenubottom 
}

TRIGGER fileOptions {
	IF (showFilemenu == 0) {
		MENU BOTTOM ADD TEXT " " FILE http://uploadpicz.com/images/5AE6QTC.bmp showhidefile 
	}
	IF (showFilemenu == 1) {
		MENU BOTTOM ADD TEXT " Exit " FILE home.bmp exitconfirm
		MENU BOTTOM ADD TEXT " Save Universe " FILE save.bmp saveconfirm
		MENU BOTTOM ADD TEXT " Load Universe " FILE load.bmp loadconfirm
		MENU BOTTOM ADD TEXT " New Universe " ELEMENT ELEMENT:newUniverse resetconfirm
		MENU BOTTOM ADD TEXT " " FILE http://uploadpicz.com/images/4YZ579L.bmp showhidefile 
	}

}

TRIGGER cursorOptions {
	IF (showCursormenu == 0) {
		MENU BOTTOM ADD TEXT " " FILE http://uploadpicz.com/images/II52MAJ.bmp showhidecursor
	}
	IF (showCursormenu == 1) {
		MENU BOTTOM ADD TEXT " " FILE http://uploadpicz.com/images/4JQNXI2.bmp showhidecursor
		MENU BOTTOM ADD TEXT " Cursor Elements " ELEMENT ELEMENT:cursorElements cursorElementMenu
		MENU BOTTOM ADD TEXT "" SEPERATOR 8 <>
		MENU BOTTOM ADD TEXT "Size" ELEMENT ELEMENT:cursorSize cursorSizeSub
		MENU BOTTOM ADD TEXT "" SEPERATOR 8 <>
		MENU BOTTOM ADD TEXT " Cursor " ELEMENT ELEMENT:cursorStyle cursorShapeSub
		MENU BOTTOM ADD TEXT "" SEPERATOR 16 <>
	}

}

TRIGGER gameOptions {
	IF (showOptionsmenu == 0) {
		MENU BOTTOM ADD TEXT " " FILE http://uploadpicz.com/images/5AE6QTC.bmp showhideoptions 
	}
	IF (showOptionsmenu == 1) {
		MENU BOTTOM ADD TEXT "" SEPERATOR 16 <>
		MENU BOTTOM ADD TEXT "Clear Screen" FILE empty.bmp <DRAW ELEMENT:Clear FILLEDRECT 0 0 (WIDTH + 2) (HEIGHT + 2)>
		MENU BOTTOM ADD TEXT "" SEPERATOR 8 <>
		IF (SPEED == 0) <MENU BOTTOM ADD TEXT "Speed" FILE pause.bmp speedmenu>
		IF ((SPEED > 0) && (SPEED < 60) ) <MENU BOTTOM ADD TEXT "Speed" FILE slow.bmp speedmenu>
		IF (SPEED == 60) <MENU BOTTOM ADD TEXT "Speed" FILE play.bmp speedmenu>
		IF (SPEED > 60) <MENU BOTTOM ADD TEXT "Speed" FILE fast.bmp speedmenu>
		MENU BOTTOM ADD TEXT "" SEPERATOR 8 <>
		MENU BOTTOM ADD TEXT " Show Arrows " ELEMENT ELEMENT:arrows toggledrawarrows
		MENU BOTTOM ADD TEXT " " FILE http://uploadpicz.com/images/4YZ579L.bmp showhideoptions 
	}

}

TRIGGER automixRe-arrangemenu {
	IF (showAutomenu == 0) {
		IF (Universeaction == 0) {
			MENU BOTTOM ADD TEXT " " FILE http://uploadpicz.com/images/II52MAJ.bmp showhideauto
		}
		IF (Universeaction != 0) {
			MENU BOTTOM ADD TEXT " " FILE http://uploadpicz.com/images/II52MAJ.bmp showhideauto
			MENU BOTTOM ADD TEXT " Stop " ELEMENT ELEMENT:stopmix exitautoaction
		}
	}
	IF (showAutomenu == 1) {
		MENU BOTTOM ADD TEXT " " FILE http://uploadpicz.com/images/4JQNXI2.bmp showhideauto
		IF (Universeaction == 0) {
			MENU BOTTOM ADD TEXT "" SEPERATOR 8 <>
			MENU BOTTOM ADD TEXT " Auto Mix " ELEMENT ELEMENT:automix mixconfirm
			MENU BOTTOM ADD TEXT "" SEPERATOR 32 <>
			MENU BOTTOM ADD TEXT " Auto Re-arrange " ELEMENT ELEMENT:autoRe-arrange Re-arrangeconfirm
			MENU BOTTOM ADD TEXT "" SEPERATOR 24 <>
		}
		IF (Universeaction != 0) {
			EXEC menushowcurrent
			IF (Universeaction == 1) <MENU BOTTOM ADD TEXT "" SEPERATOR 16 <>>
			IF (Mspeed <= maxspeed) <MENU BOTTOM ADD TEXT " " ELEMENT ELEMENT:turboOFFicon turboswitch>
			IF (Mspeed == turbospeed) <MENU BOTTOM ADD TEXT " " ELEMENT ELEMENT:turboLOWicon turboswitch>
			IF (Mspeed == turbo2speed) <MENU BOTTOM ADD TEXT " " ELEMENT ELEMENT:turboHIGHicon turboswitch>
			IF (Universeaction == 1) <MENU BOTTOM ADD TEXT "" SEPERATOR 16 <>>
			IF (Universeaction != 1) {
				IF (repeatRe-arrange == 0) <MENU BOTTOM ADD TEXT " " ELEMENT ELEMENT:repeatofficon repeattoggle>
				IF (repeatRe-arrange == 1) <MENU BOTTOM ADD TEXT " " ELEMENT ELEMENT:repeatonicon repeattoggle>
			}	
			MENU BOTTOM ADD TEXT " Stop " ELEMENT ELEMENT:stopmix exitautoaction
		}
	}
}

TRIGGER menushowcurrent {
	IF ((Universeaction == 1) | (Universeaction == 4)) <MENU BOTTOM ADD TEXT " " ELEMENT ELEMENT:mixingicon <>>
	IF ((Universeaction == 2) | (Universeaction == 3)) <MENU BOTTOM ADD TEXT " " ELEMENT ELEMENT:solvingicon <>>
	IF (Universeaction == 5) <MENU BOTTOM ADD TEXT " " ELEMENT ELEMENT:Re-arrangedicon <>>
}

ON defaultstatus IF (statustype == 3) {
	STATUS CLEAR
	STATUS ADDTEXT "  Auto Re-arranged:" 
	STATUS ADDNUMBER puzzleRe-arranged  
	STATUS ADDTEXT "  Moves:" 
	STATUS ADDNUMBER autoRe-arrangesteps 
	STATUS ADDTEXT "  Avg:" 
	STATUS ADDNUMBER avgstepstoRe-arrange 
	STATUS ADDTEXT "  Min:" 
	STATUS ADDNUMBER minstepstoRe-arrange 
	STATUS ADDTEXT "  Max:" 
	STATUS ADDNUMBER maxstepstoRe-arrange
	IF (Universeaction != 4) {
		STATUS ADDTEXT "  Progress:" 
		STATUS ADDNUMBER (ASprogress * 10) 
		STATUS ADDTEXT "% " 
	}
	IF (Universeaction == 4) {
		STATUS ADDTEXT "  Remixing:" 
		STATUS ADDNUMBER remixcycles
	}
}

