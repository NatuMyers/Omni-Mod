TRIGGER SpecialBoom1 {
	DRAW ELEMENT:Fire FILLEDCIRCLE $0 $1 1
	IF (($0 > 0) && ($0 < WIDTH) && ($1 < HEIGHT)) {
		TIMER 1 FRAMES SpecialBoom1 ($0 + $2) ($1 - $3) $2 ($3 - 1)
	}
}

// ShipFuel-ish
TRIGGER SpecialBoom2 {
	DRAW ELEMENT:FireWave CIRCLE $0 $1 $2
	IF ($2 < $3) {
		TIMER 1 FRAMES SpecialBoom2 $0 $1 ($2 + 1) $3
	}
}

// EmberFlare-ish
TRIGGER SpecialBoom3 {
	DRAW ELEMENT:BurningEmberFlare FILLEDCIRCLE $0 $1 2
	DRAW ELEMENT:Fire FILLEDCIRCLE ($0 + (-3 RAND 3)) ($1 + (-3 RAND 3)) 1
	DRAW ELEMENT:Fire FILLEDCIRCLE ($0 + (-3 RAND 3)) ($1 + (-3 RAND 3)) 1
	DRAW ELEMENT:Fire FILLEDCIRCLE ($0 + (-3 RAND 3)) ($1 + (-3 RAND 3)) 1
	DRAW ELEMENT:Fire FILLEDCIRCLE ($0 + (-3 RAND 3)) ($1 + (-3 RAND 3)) 1
	DRAW ELEMENT:Fire FILLEDCIRCLE ($0 + (-3 RAND 3)) ($1 + (-3 RAND 3)) 1
	DRAW ELEMENT:Fire FILLEDCIRCLE ($0 + (-3 RAND 3)) ($1 + (-3 RAND 3)) 1
	IF ($4 > 0) {
		TIMER 1 FRAMES SpecialBoom3 ($0 + $2) ($1 + $3) (-3 RAND 3) (-3 RAND 3) ($4 - 1)
	}
}

// ShipOil-ish
TRIGGER SpecialBoom4 {
	DRAW ELEMENT:Fire FILLEDCIRCLE $0 $1 $2
	IF ($2 > $3) {
		TIMER 1 FRAMES SpecialBoom4 $0 ($1 - $4) ($2 - 1) $3 $4
	}
}

// ElectricBurst-ish
TRIGGER SpecialBoom5 {
	DRAW ELEMENT:Na1 POINT $0 $1
	IF (($0 > 0) && ($0 < WIDTH) && ($1 > 0) && ($1 < HEIGHT)) {
		TIMER 1 FRAMES SpecialBoom5 ($0 + ($2 * 3)) ($1 - ($3 * 3)) $2 $3
	}
}

// Cluster MassDestruction
TRIGGER SpecialBoomC1 {
	DRAW ELEMENT:Cluster1 FILLEDCIRCLE $0 $1 $2
	IF ($2 < $3) {
		TIMER 1 FRAMES SpecialBoomC1 $0 $1 ($2 + 1) $3
	}
}

// Flash Bang
TRIGGER flash {
	DRAW ELEMENT:TheLight REPLACEFILLEDCIRCLE $0 $1 $2 0 ELEMENT:Clear
	DRAW ELEMENT:TheLight REPLACEFILLEDCIRCLE $0 $1 $2 0 ELEMENT:HyperLucent
	IF ($2 < $3) <TIMER 1 FRAMES flash $0 $1 ($2 + 4) $3>
}

// Hydrogen MassDestruction
TRIGGER SpecialBoomH1 {
	TIMER 1 FRAMES SpecialBoomH2 $0 $1 1
}
TRIGGER SpecialBoomH2 {
	IF ($1 > 0) {
		DRAW ELEMENT:H-Blast1 FILLEDCIRCLE $0 $1 5
		TIMER 1 FRAMES SpecialBoomH2 $0 ($1 - $2) ($2 + 1)
	}
	IF ($1 <= 0) {
		TIMER 1 FRAMES SpecialBoomH3 ($0 + (-50 RAND 50)) (0 RAND 50) 50
	}
}
TRIGGER SpecialBoomH3 {
	IF ($2 > 0) {
		TIMER 1 FRAMES SpecialBoomH4 $0 $1 (-10 RAND 10) (0 RAND 10)
		TIMER 1 FRAMES SpecialBoomH3 ($0 + (-50 RAND 50)) (0 RAND 50) ($2 - 1)
	}
}
TRIGGER SpecialBoomH4 {
	DRAW ELEMENT:H-Blast2 FILLEDCIRCLE $0 $1 2
	IF (($0 > 0) && ($0 < WIDTH) && ($1 < HEIGHT)) {
		TIMER 1 FRAMES SpecialBoomH4 ($0 + $2) ($1 - $3) $2 ($3 - 1)
	}
	CLOSEST ELEMENT:Clear $0 $1 ($2 * $3)
	IF (CLOSESTD < ($2 * $3)) {
		DRAW ELEMENT:FallOut FILLEDCIRCLE CLOSESTX CLOSESTY 1
	}
}

// Antimatter MassDestruction
TRIGGER SpecialBoomA1 {
	DRAW ELEMENT:TheLight FILLEDCIRCLE $0 $1 $2
	IF ($2 < $3) {
		TIMER 1 FRAMES SpecialBoomA1 $0 $1 ($2 + 2) $3
	}
	IF ($2 >= $3) {
		TIMER 1 FRAMES SpecialBoomA2 $0 $1 $2 1
	}
}
TRIGGER SpecialBoomA2 {
	DRAW ELEMENT:TheLight FILLEDCIRCLE $0 $1 $2
	IF ($2 > $3) {
		TIMER 1 FRAMES SpecialBoomA2 $0 $1 ($2 - 2) $3
	}
	IF ($2 <= $3) {

		TIMER 1 FRAMES SpecialBoomA3 250 0
	}
}
TRIGGER SpecialBoomA3 {
	IF ($0 > 0) {
		TIMER 1 FRAMES SpecialBoomA3 ($0 - 1)
		TIMER (1 RAND 10) FRAMES SpecialBoomA4 (0 RAND WIDTH) (0 RAND HEIGHT) 1 (10 RAND 100)
	}
	IF ($0 <= 0) {
		TIMER 1 FRAMES SpecialBoomA5
	}
}
TRIGGER SpecialBoomA4 {
	DRAW ELEMENT:TheLight CIRCLE $0 $1 $2
	IF (useplane == 1) <DRAW ELEMENT:Fire FILLEDCIRCLE (planex + (0 RAND 50)) (planey + (0 RAND 25)) 10>
	IF ($2 < $3) {
		TIMER 1 FRAMES SpecialBoomA4 $0 $1 ($2 + 2) $3
	}
}
